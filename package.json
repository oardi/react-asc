{
	"name": "react-asc",
	"version": "26.0.0",
	"engines": {
		"npm": ">=10.9.0",
		"node": ">=22.11.0"
	},
	"description": "handcrafted react components",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"jsnext:main": "dist/index.es.js",
	"author": "Ardian Shala",
	"homepage": "https://react-asc.netlify.app",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"start": "vite --config src/app/vite.config.ts --open",
		"build": "vite build --config src/app/vite.config.ts",
		"build:lib": "rimraf dist && vite build --config src/lib/vite.config.ts && tsc --project src/lib/tsconfig.json",
		"postbuild": "node copyfiles-build.js",
		"postbuild:lib": "node copyfiles-lib.js",
		"preview": "vite preview",
		"lint": "eslint --ext ts,tsx src && eslint --ext ts,tsx lib",
		"deploy:lib:local": "node deploy-local.js"
	},
	"dependencies": {
		"@popperjs/core": "2.11.8",
		"axios": "^1.8.3",
		"dayjs": "1.11.13",
		"file-saver": "^2.0.5",
		"modern-normalize": "3.0.1",
		"papaparse": "^5.5.2",
		"prismjs": "^1.30.0",
		"react": "19.0.0",
		"react-calendar": "5.1.0",
		"react-dom": "19.0.0",
		"react-router-dom": "^7.3.0",
		"snarkdown": "2.0.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.0",
		"@eslint/js": "^9.21.0",
		"@testing-library/jest-dom": "6.6.3",
		"@testing-library/react": "16.2.0",
		"@testing-library/user-event": "14.6.1",
		"@types/file-saver": "^2.0.7",
		"@types/jest": "29.5.14",
		"@types/node": "^22.13.10",
		"@types/papaparse": "^5.3.15",
		"@types/prismjs": "1.26.5",
		"@types/react": "19.0.10",
		"@types/react-dom": "19.0.4",
		"@types/react-router-dom": "5.3.3",
		"@typescript-eslint/eslint-plugin": "8.24.0",
		"@typescript-eslint/parser": "8.24.0",
		"@vitejs/plugin-react": "4.3.4",
		"eslint": "^9.20.0",
		"eslint-plugin-react": "^7.37.4",
		"globals": "^16.0.0",
		"rimraf": "6.0.1",
		"sass": "1.85.1",
		"shelljs": "^0.8.5",
		"typescript": "5.7.3",
		"typescript-eslint": "^8.25.0",
		"vite": "^6.2.2",
